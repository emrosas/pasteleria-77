#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/emireles/git/pasteleria-77/node_modules/.pnpm/astro@5.0.0-beta.7_@types+node@22.5.5_jiti@1.21.6_rollup@4.25.0_typescript@5.6.2_yaml@2.5.1/node_modules/astro/node_modules:/Users/emireles/git/pasteleria-77/node_modules/.pnpm/astro@5.0.0-beta.7_@types+node@22.5.5_jiti@1.21.6_rollup@4.25.0_typescript@5.6.2_yaml@2.5.1/node_modules:/Users/emireles/git/pasteleria-77/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/emireles/git/pasteleria-77/node_modules/.pnpm/astro@5.0.0-beta.7_@types+node@22.5.5_jiti@1.21.6_rollup@4.25.0_typescript@5.6.2_yaml@2.5.1/node_modules/astro/node_modules:/Users/emireles/git/pasteleria-77/node_modules/.pnpm/astro@5.0.0-beta.7_@types+node@22.5.5_jiti@1.21.6_rollup@4.25.0_typescript@5.6.2_yaml@2.5.1/node_modules:/Users/emireles/git/pasteleria-77/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
